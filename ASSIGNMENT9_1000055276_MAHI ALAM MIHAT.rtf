import java.util.Scanner;\
\
public class mihat\{\
    public static void main(String []args)\
    \{\
        Scanner sc =new Scanner (System.in);\
        int N= sc.nextInt();\
        int x =fibonacci(N);\
        System.out.print(x);\
    \}\
    public static int fibonacci (int n)\
    \{\
        if (n<=1)\
        \{\
            return n;\
        \}\
        else\{\
            return fibonacci(n-1) + fibonacci(n-2);\
        \}\
    \}\
\}\
