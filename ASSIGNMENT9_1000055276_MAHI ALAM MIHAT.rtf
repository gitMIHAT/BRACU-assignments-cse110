public class mihat\{\
    public static void main(String []args)\
    \{\
        Scanner sc =new Scanner (System.in);\
        int N= sc.nextInt();\
        int x =factorial(N);\
        System.out.print(x);\
    \}\
    public static int factorial (int a)\
    \{\
        if (a==1)\
        \{\
            return 1;\
        \}\
        else\{\
            return a*factorial(a-1);\
        \}\
    \}\
\}\
