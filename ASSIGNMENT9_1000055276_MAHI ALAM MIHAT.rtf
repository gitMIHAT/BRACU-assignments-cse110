    public static void main(String []args)\
    \{\
        Scanner sc =new Scanner (System.in);\
        int n= sc.nextInt();//5\
        int m= sc.nextInt();//3\
        int x = power(n,m);\
        System.out.println(x);\
    \}\
    public static int power(int n,int m) \
    \{\
        if (m<=0)\
        \{\
            return 1;\
        \}\
        else\{\
            return n* power(n ,m-1); \
        \}\
    \}\
\}\
